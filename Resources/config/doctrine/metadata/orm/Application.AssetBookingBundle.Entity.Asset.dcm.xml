<!-- Application/HelloBundle/Resources/config/doctrine/metadata/orm/Application.HelloBundle.Entity.User.dcm.xml -->
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\AssetBookingBundle\Entity\Asset" table="asset">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>

        <field name="availableFrom" column="available_from" type="datetime" />
        <field name="availableTo" column="available_to" type="datetime" />

        <field name="name" column="name" type="string" length="100" />

        <field name="createdAt" column="created_at" type="datetime" />
        <field name="updatedAt" column="updated_at" type="datetime" />

    </entity>


</doctrine-mapping>