<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\AssetBookingBundle\Entity\StatusProfileItem" table="status_profile_item">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>

        <one-to-one field="status" target-entity="Status" inversed-by="status">
               <join-column name="status_id" referenced-column-name="id" on-delete="CASCADE" on-update="CASCADE"/>
        </one-to-one>


        <field name="sequenceNumber" column="sequence_number" type="integer" />
       
        <field name="createdAt" column="created_at" type="datetime" />
        <field name="updatedAt" column="updated_at" type="datetime" />

    </entity>


</doctrine-mapping>